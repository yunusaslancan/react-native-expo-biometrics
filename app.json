{
  "expo": {
    "name": "biometrics",
    "slug": "biometrics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSFaceIDUsageDescription": "We need your FaceID and TouchID to authenticate you in the app."
      },
      "bundleIdentifier": "com.biometrics"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.biometrics",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "USE_BIOMETRIC"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b5f660fb-ef64-40dc-a470-cc2ca24f7525"
      }
    }
  }
}
